{
	"name": "@ysyp/template",
	"version": "0.1.0",
	"private": true,
	"description": "Taro template for YuanshengYoupin",
	"templateInfo": {
		"name": "mobx",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:weapp:upload": "taro build --type weapp --upload",
		"build:weapp:preview": "taro build --type weapp --preview",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:kwai": "taro build --type kwai",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:kwai": "npm run build:kwai -- --watch",
		"dev:h5": "SERVER_ENV=ysyp && npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"export": "./node_modules/.bin/next export",
		"build": "./node_modules/.bin/next build",
		"release": "np"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "Fu Yin",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@tarojs/components": "3.3.9",
		"@tarojs/plugin-html": "^3.3.9",
		"@tarojs/plugin-platform-kwai": "^1.0.0",
		"@tarojs/react": "3.3.9",
		"@tarojs/rn-runner": "^3.3.9",
		"@tarojs/runtime": "3.3.9",
		"@tarojs/taro": "3.3.9",
		"@tarojs/taro-rn": "^3.3.9",
		"@tarojs/taro-weapp": "^2.2.10",
		"@ysyp/stores": "latest",
		"@ysyp/ui": "latest",
		"@ysyp/utils": "latest",
		"fs": "0.0.1-security",
		"lodash.mapvalues": "^4.6.0",
		"mobx": "^6.3.2",
		"mobx-react": "^7.2.0",
		"mobx-state-tree": "^5.0.2",
		"mobx-store-provider": "^2.1.1",
		"net": "^1.0.2",
		"react": "^17.0.0",
		"react-dom": "^17.0.2",
		"react-merge-refs": "^1.1.0",
		"react-native": "^0.64.0",
		"react-use-measure": "^2.0.4",
		"screenshot": "^0.1.0",
		"tls": "0.0.1",
		"use-asset": "^1.0.4",
		"zustand": "^3.5.7"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/mini-runner": "3.3.9",
		"@tarojs/webpack-runner": "3.3.9",
		"@types/react": "^17.0.2",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^4.15.1",
		"@typescript-eslint/parser": "^4.15.1",
		"babel-plugin-import": "^1.13.3",
		"babel-preset-taro": "3.3.9",
		"eslint": "^6.8.0",
		"eslint-config-next": "^11.0.1",
		"eslint-config-taro": "3.3.9",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"path": "^0.12.7",
		"stylelint": "^9.10.1",
		"typescript": "^4.1.0",
		"webpack-bundle-analyzer": "^4.4.2"
	},
	"np": {
		"yarn": false
	}
}
